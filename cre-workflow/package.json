{
  "name": "liquidbridge-cre-workflow",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start:standalone": "npx tsx src/standalone/nav-updater.ts",
    "dev:standalone": "npx tsx --watch src/standalone/nav-updater.ts",
    "start:mock-api": "npx tsx src/mock-api/server.ts",
    "simulate": "cre workflow simulate src/nav-updater-workflow --target staging-settings",
    "simulate:broadcast": "cre workflow simulate src/nav-updater-workflow --target staging-settings --broadcast"
  },
  "dependencies": {
    "@chainlink/cre-sdk": "^1.0.7",
    "viem": "^2.0.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
